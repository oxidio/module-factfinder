{
  "name": "oxidio/module-factfinder",
  "type": "oxideshop-module",
  "description": "factfinder module for oxid shop",
  "license": "GPL-3.0",
  "keywords": [
    "fact finder",
    "module",
    "oxid"
  ],
  "require": {
    "fact-finder/fact-finder-php-library": "^1",
    "php": ">=7.1"
  },
  "require-dev": {
    "oxidio/project": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Oxidio\\": ["src/Oxidio/"]
    },
    "files": [
      "src/autoload-files.php"
    ]
  },
  "extra": {
    "oxideshop": {
      "whitelist-filter": [
        "metadata.php"
      ],
      "target-directory": "oxidio/factfinder"
    },
  }
}
